  <head>
     <meta charset="utf-8">
      <title><%= title %> - Ranking games based on twitter buzz</title>
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
      <meta name="description" content="">
      <meta name="author" content="">

    <link href="/stylesheets/foundation.css" rel="stylesheet">
    <link href="/stylesheets/style.css" rel="stylesheet">
    <link href="/stylesheets/fontello.css" rel="stylesheet">
    <link rel="shortcut icon" href="/images/icon.ico" />
    <link href='http://fonts.googleapis.com/css?family=Dosis:400,500,600' rel='stylesheet' type='text/css'>

    <script src="/js/vendor/custom.modernizr.js"></script>

     
  </head>
  <body style="display:none;" onunload="">
  <div class="row">
    <div class="large-12 columns">
    <!-- header area -->
    <% include includes/header.ejs %>
  <!-- end of header area -->
            
    <!-- Page / ranking special -->
    <div class="row">
      <h2 class="bigTitle pageHeader"><a href="/list/main" rel="fade"><!--<span data-tooltip class="has-tip" title="The most popular current games accross all genres and platforms, ranked by number of mentions on Twitter and soon Youtube views and mentions accross the interweb.  Games are defined as current if they have been released in the last 2 months or are slated for release in the future">--><%= rankingTitle %><!--</span>--></a> </h2>
    </div>


    <!-- Content sectiong -->
    <div class="row">


    <!-- 
    Row 1 contains 3
    Row 2 - 4 contain 6
    Row 5 - end contains 8?
    -->

      <% 

      function determineTrend(previous){
        var diff = previous;

        console.log(previous)

        if (diff == 0) {
          return "icon-ellipsis";
        } else if (diff > 0) {
          return "icon-down-big"

        } else if (diff < 0) {
          return "icon-up-big"
        }

      }
      var titleClass = "gametitle";
      for (var i=0; i < data.length; i++){ %>


      <% if ( i == 0 ) { %>
        <div class="row">
        
          <div class="large-6 small-6 columns">
      <% } %>

      <% if ( i == 1 ) { %>
          <div class="large-6 small-6 columns">
          <div class="row">
      <% } %>

      <% if ( i > 0 && i < 5 ) { %>
          <div class="large-6 small-6 columns">

      <% } %>

      <% if ( i == 5 ) { 
        var newcount = 0;
        newcount++; %>
        <div class="row">
      <% } %>

      <% if ( i >= 5 ) {
        titleClass = "gametitle-small"; %>
        <div class="large-2 small-4 columns equalheigth">
      <% } %>
<div class="rectangle label" style="float:left; position:relative; top:40px; left:20px; font-size: 1.0em"><%= i+1 %></div>
<div class="rectangle label" style="float:left; position:absolute; top:40px; left:60px; font-size:1.0em; background-color: black"><i class="<%= determineTrend(data[i].get('delta_1_recent_rank')) %>"></i></div>

<!--<a href="/details/<%= data[i]._id %>" class="details-link" data-group="rel" data-id="<%= data[i]._id %>"><img src="<%= data[i].get('thumbUrl') %>" height="245" style="width:100%;"/></a>-->

<!-- -->
<a href="/details/<%= data[i]._id %>" rel="fade">
    <img src="<%= data[i].get('thumbUrl') %>" height="245" style="width:100%;"/></a>
</a>

  
        <div class="row">&nbsp;</div>


</div>




      <% if ( i == 2 ) { %>
        </div>
        <div class="row">
      <% } %>

      <% if ( i == 4 ) { %>
        </div>
        </div>
      <% } %>

      <% if ( newcount % 6 == 0 ) { %>
      </div>
      <div class="row">
      <% } %>





<!-- End card content -->


<% } %>


    <h6 class="moreTitle"><a href="/list/main" rel="fade">EXPLORE THE WHAT'S POPULAR GAME RANKINGS</a></h6>
    </div>
    </div>
    <div class="lightgray" style="width: 100%;">
      <div class="row">
        <h2 class="bigTitle"><a href="/list/kickstarter" rel="fade"><!--<span data-tooltip class="has-tip" title="The most popular Kickstarter projects.  Ranked by number of mentions on Twitter and soon Youtube views and mentions accross the interweb.  Projects tracked need not be funded, but are removed from the index when funding goal is not reached">-->TRENDING KICKSTARTER-FUNDED GAMES<!--</span>--></a></h2>
      </div>
      <!-- Content sectiong Kickstarter-->

      <div class="row">
        <ul class="small-block-grid-3 large-block-grid-5">
        <% 
          for (var i=0; i < kick.length; i++){ 
        %>
          
            <li>
              <div class="item">                
                <a href="/details/<%= kick[i]._id %>" rel="fade">
                 <div class="rectangle label" style="float:left; position:absolute; top:20px; left:10px; font-size:1.1em"><%= i+1 %></div>
                 <div class="rectangle label" style="float:left; position:absolute; top:20px; left:40px; font-size:1.1em; background-color: black;"><i class="<%= determineTrend(kick[i].get('delta_1_recent_rank')) %>"></i></div>
                  <div class="item-billboard">
                    <img src="<%= kick[i].get('thumbUrl') %>" height="245" style="width:100%; vertical-align:top;"/>
                  </div>

                  <div class="small-12 columns <%= titleClass %>" style="padding-top: 30px;"><h6><%= kick[i].title %></h6><% if (kick[i].title.length < 24) { %><br><% } %></div>

                  <div class="item-detail">
                      <div class="container">
                        <div class="row">
                          <div class="small-12 columns <%= titleClass %>">
                              <p><strong><%= kick[i].trendIndex %></strong> Mentions<br />Launches <%= String(kick[i].releaseDate).substring(0,15) %></p>
                              <p><%= kick[i].get('description').substring(0,140) %>... <% var str = kick[i].get('description'); %></p> 
                            
                          </div>
                        </div>
                      </div>


                    </div> 
                  
                </a>
              </div>
            </li>
          
        <%
         }
        %>
        </ul>
         <h6 class="moreTitle"><a href="/list/kickstarter" rel="fade">EXPLORE THE KICKSTARTER-FUNDED GAME RANKINGS</a></h6>
      </div>
    </div>

    <div class="darkgray">
      <div class="row">
        <h2 class="bigTitle"><a href="/list/mobile" rel="fade"><!--<span data-tooltip class="has-tip" title="The most popular current mobile games accross all genres and mobile platforms, ranked by number of mentions on Twitter and soon Youtube views and mentions accross the interweb.  Games are defined as current if they have been released in the last 2 months or are slated for release in the future">-->MOST POPULAR MOBILE GAMES<!--</span>--></a></h2>
      </div>
      <!-- Content section Mobile -->

      <div class="row">
        
        <ul class="small-block-grid-4 large-block-grid-7">
        <% 
          for (var i=0; i < mobile.length; i++){ 
        %>
          
            <li>
              <div class="itemsmall">
                <a href="/details/<%= mobile[i]._id %>" rel="fade">
                <div class="rectangle label" style="float:left; position:absolute; top:10px; left:10px; font-size:0.9em"><%= i+1 %></div>
                <div class="rectangle label" style="float:left; position:absolute; top:10px; left:40px; font-size:0.9em; background-color: black;"><i class="<%= determineTrend(mobile[i].get('delta_1_recent_rank')) %>"></i></div>
                  <div class="itemsmall-billboard">
                    <img src="<%= mobile[i].get('thumbUrl') %>" height="245" style="width:100%;"/>
                  </div>
                  <div class="small-12 columns <%= titleClass %>" style="padding-top:;"><h6><%= mobile[i].title %></h6><% if (mobile[i].title.length < 21) { %><br><% } %></div>

                  <div class="itemsmall-detail">
                      <div class="container">
                        <div class="row">
                          <div class="small-12 columns <%= titleClass %>">
                              <p><strong><%= mobile[i].trendIndex %></strong> Mentions<br />Goes Live <%= String(mobile[i].releaseDate).substring(0,15) %></p>
                              <p><%= mobile[i].get('description').substring(0,100) %>... <% var str = mobile[i].get('description'); console.log("the description will be ", str.substring(0,140), "...") %></p> 
                            
                          </div>
                        </div>
                      </div>


                    </div> 
                  
                </a>
              </div>
            </li>
          
        <%
         }
        %>
        </ul>
        <h6 class="moreTitle"><a href="/list/mobile" rel="fade">EXPLORE THE MOBILE GAME RANKINGS</a></h6>

      </div>
    </div> 
   <!-- footer are -->
    <% include includes/footer.ejs %>
  <!-- end of footer area -->




